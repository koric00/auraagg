# Server Configuration
PORT=3000
NODE_ENV=development
REQUIRE_API_KEY_IN_DEV=false

# Redis Configuration
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=
REDIS_PREFIX=auraagg:

# API Keys (comma-separated)
API_KEYS=test-api-key-1,test-api-key-2

# RPC Endpoints
ETH_RPC_URL=https://mainnet.infura.io/v3/your-infura-key
ARBITRUM_RPC_URL=https://arb1.arbitrum.io/rpc
OPTIMISM_RPC_URL=https://mainnet.optimism.io
POLYGON_RPC_URL=https://polygon-rpc.com
SOLANA_RPC_URL=https://api.mainnet-beta.solana.com

# Contract Addresses
ROUTER_CONTRACT_ETH=0x1234567890123456789012345678901234567890
ROUTER_CONTRACT_ARBITRUM=0x1234567890123456789012345678901234567890
ROUTER_CONTRACT_OPTIMISM=0x1234567890123456789012345678901234567890
ROUTER_CONTRACT_POLYGON=0x1234567890123456789012345678901234567890

# External APIs
COINGECKO_API_KEY=your-coingecko-api-key
ETHERSCAN_API_KEY=your-etherscan-api-key
ARBISCAN_API_KEY=your-arbiscan-api-key
OPTIMISM_EXPLORER_API_KEY=your-optimism-explorer-api-key
POLYGONSCAN_API_KEY=your-polygonscan-api-key

# Logging
LOG_LEVEL=info
LOG_FILE_MAX_SIZE=5242880
LOG_MAX_FILES=5

# Rate Limiting
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=100

# Cache Settings
CACHE_TTL=60 